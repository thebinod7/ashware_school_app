<div class="main-section report-panel main-account-settings">
  <!-- <div class="ui-header">Reports</div> -->
  <div class="use-grid use-2x-partial">
    <div class="ui-dashbox">
      <div class="ui-top-db-twin">
        <div class="db-twin-child">
          <h3>Coming soon</h3>
        </div>
        <div class="db-twin-child">
          <!-- <div class="uk-inline">
                                <span class="uk-form-icon" uk-icon="icon: search"></span>
                                <input class="uk-input ui-input-curve" type="text" placeholder="Search for anything...">
                            </div> -->
        </div>
      </div>
      <div class="ui-top-middle-db-grid use-grid use-6x">
        <span>name</span>
        <span>code</span>
        <span>pages</span>
        <span>cookies</span>
        <span>page views</span>
        <span>actions</span>
      </div>
      <div class="ui-bottom-db">
        <div class="ui-bottom-db-empty">
          <div class="db-img-wrapper">
            <img src="/assets/Svg/list.svg" />
          </div>
          <h4>Coming soon</h4>
          <p>Successful actions received will be plotted on a chart here.</p>
          <a href="#" class="ui-link"
            >Invite members
            <ion-icon name="arrow-forward-circle-outline"></ion-icon
          ></a>
        </div>
      </div>
    </div>

    <% if(user.role !== 'Parent') { %>
    <div>
      <div class="ui-dashbox">
        <div class="ui-top-db-twin">
          <div class="db-twin-child">
            <h3>Coming soon</h3>
          </div>
          <div class="db-twin-child"></div>
        </div>
        <div class="ui-bottom-db ui-bottom-db--details">
          <p>Total student</p>
          <h1>30</h1>
        </div>
      </div>
      <div class="ui-dashbox">
        <div class="ui-top-db-twin">
          <div class="db-twin-child">
            <h3>Active Domains</h3>
          </div>
          <div class="db-twin-child"></div>
        </div>
        <div class="ui-bottom-db ui-bottom-db--details">
          <p>Total student</p>
          <h1>30</h1>
        </div>
      </div>
    </div>

    <% } %>
  </div>
</div>

